{
  "name": "henrike-legner",
  "version": "2.0.0",
  "author": "Philipp Legner",
  "source": "src/index.pug",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "scripts": {
    "start": "parcel src/*.pug -p 5000 --open chrome",
    "build": "rm -rf dist && parcel build src/*.pug --no-source-maps && rm dist/_layout.html && cp src/CNAME dist",
    "deploy": "npm run build && push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "jstransformer-markdown-it": "^3.0.0",
    "parcel": "^2.8.2",
    "push-dir": "^0.4.1"
  }
}
